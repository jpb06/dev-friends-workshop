{
  "name": "front",
  "author": "jpb06",
  "license": "MIT",
  "version": "1.0.0",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start -p $PORT",
    "format": "prettier --write .",
    "lint": "next lint --dir ./src",
    "type-check": "tsc --pretty --noEmit --pretty",
    "test": "jest",
    "test:dev": "jest --watchAll --coverage",
    "test:ci": "jest --runInBand --ci --coverage",
    "test-all": "yarn lint && yarn type-check && yarn test",
    "postinstall": "husky install",
    "api:sync": "generateTypesFromUrl NEXT_PUBLIC_API_URL -json ./src/api/main-backend/specs && eslint ./src/api/main-backend/specs --fix"
  },
  "engines": {
    "node": "16.x",
    "yarn": "1.22.x"
  },
  "dependencies": {
    "@emotion/react": "^11.8.2",
    "@emotion/styled": "^11.8.1",
    "@mui/icons-material": "^5.5.1",
    "@mui/material": "^5.5.2",
    "axios": "^0.27.2",
    "next": "12.2.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-query": "^4.0.0-beta.3"
  },
  "devDependencies": {
    "@swc/core": "^1.2.208",
    "@swc/jest": "^0.2.21",
    "@testing-library/jest-dom": "^5.16.3",
    "@testing-library/react": "^13.1.1",
    "@testing-library/user-event": "^14.1.0",
    "@types/jest": "^28.1.4",
    "@types/jest-when": "^3.5.0",
    "@types/node": "^18.0.1",
    "@types/react": "^18.0.5",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "babel-jest": "^28.1.2",
    "eslint": "^8.12.0",
    "eslint-config-next": "^12.1.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest": "^26.1.3",
    "eslint-plugin-jest-dom": "^4.0.1",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^8.0.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^28.1.2",
    "jest-environment-jsdom": "^28.1.2",
    "jest-watch-typeahead": "^1.0.0",
    "jest-when": "^3.4.0",
    "msw": "0.43.0",
    "prettier": "^2.6.1",
    "swagger-typescript-types": "^3.0.0",
    "typescript": "^4.6.3"
  }
