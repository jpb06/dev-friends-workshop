  "license": "MIT",
  "version": "1.0.0",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start -p $PORT",
    "format": "prettier --write .",
    "lint": "next lint --dir ./src",
    "type-check": "tsc --pretty --noEmit --pretty",
    "test": "vitest",
    "test-dev": "vitest --watch --coverage",
    "test-ci": "vitest --runInBand --ci --coverage",
    "test-all": "pnpm lint && pnpm type-check && pnpm test",
    "postinstall": "husky install",
    "sync-api": "generateTypesFromUrl -u https://devfriends-backend.fly.dev/-json -o ./src/api/main-backend/specs && eslint ./src/api/main-backend/specs --fix",
    "sync-icons": "pnpm generateReadmeIcons -h 50"
  },
  "engines": {
    "node": "20.x"
  },
  "dependencies": {
    "@emotion/react": "^11.10.0",
    "@emotion/styled": "^11.10.0",
    "@mui/icons-material": "^5.15.2",
    "@mui/material": "^5.15.2",
    "@mui/system": "^5.15.2",
    "@tanstack/react-query": "^5.14.6",
    "axios": "^1.2.4",
    "jotai": "^2.6.0",
    "next": "14.0.4",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "ts-pattern": "^5.0.6"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@emotion/cache": "^11.10.5",
    "@faker-js/faker": "^8.3.1",
    "@next/env": "^14.0.4",
    "@stylistic/eslint-plugin": "^1.5.1",
    "@swc/core": "^1.3.28",
    "@testing-library/dom": "^9.3.3",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.4.0",
    "@types/node": "^20.10.5",
    "@types/react": "^18.0.27",
    "@typescript-eslint/eslint-plugin": "^6.16.0",
    "@typescript-eslint/parser": "^6.16.0",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^1.1.0",
    "eslint": "^8.32.0",
    "eslint-config-next": "^14.0.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-markdown": "^3.0.1",
    "eslint-plugin-prettier": "^5.1.2",
    "eslint-plugin-testing-library": "^6.2.0",
    "husky": "^8.0.3",
    "identity-obj-proxy": "^3.0.0",
    "jsdom": "^23.0.1",
    "msw": "2.0.11",
    "prettier": "^3.1.1",
    "readme-package-icons": "^1.1.7",
    "swagger-typescript-types": "^3.0.8",
    "typescript": "^5.3.3",
    "vite": "^5.0.10",
    "vite-tsconfig-paths": "^4.2.2",
    "vitest": "^1.1.0"
  }
