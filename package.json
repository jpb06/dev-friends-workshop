    "prettier": "^3.2.5",
    "readme-package-icons": "^1.1.14",
    "swagger-typescript-types": "^3.0.8",
    "typescript": "^5.4.5",
    "vite": "^5.2.11",
    "vite-tsconfig-paths": "^4.3.2",
    "vitest": "^1.6.0"
  }
