{
  "name": "front",
  "author": "jpb06",
  "license": "MIT",
  "version": "1.0.0",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start -p $PORT",
    "format": "prettier --write .",
    "lint": "next lint --dir ./src",
    "type-check": "tsc --pretty --noEmit --pretty",
    "test": "jest",
    "test:dev": "jest --watchAll --coverage",
    "test:ci": "jest --runInBand --ci --coverage",
    "test-all": "yarn lint && yarn type-check && yarn test",
    "postinstall": "husky install",
    "api:sync": "generateTypesFromUrl NEXT_PUBLIC_API_URL -json ./src/api/main-backend/specs && eslint ./src/api/main-backend/specs --fix"
  },
  "engines": {
    "node": "14.x",
    "yarn": "1.22.x"
  },
  "dependencies": {
    "@emotion/react": "^11.8.2",
    "@emotion/styled": "^11.8.1",
    "@mui/icons-material": "^5.5.1",
    "@mui/material": "^5.5.2",
    "axios": "^0.26.1",
    "next": "12.1.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-query": "^3.24.4"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.1.0",
    "@testing-library/react-hooks": "^7.0.2",
    "@testing-library/user-event": "^13.2.1",
    "@types/jest": "^27.0.2",
    "@types/jest-when": "^3.5.0",
    "@types/node": "^17.0.22",
    "@types/react": "^17.0.24",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "babel-jest": "^27.2.2",
    "eslint": "^8.11.0",
    "eslint-config-next": "^12.1.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest": "^26.1.3",
    "eslint-plugin-jest-dom": "^4.0.1",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.2",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.2.2",
    "jest-watch-typeahead": "^1.0.0",
    "jest-when": "^3.4.0",
    "msw": "^0.39.2",
    "prettier": "^2.4.1",
    "swagger-typescript-types": "^2.1.7",
    "typescript": "^4.4.3"
  }
}
