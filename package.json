  "version": "1.0.0",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start -p $PORT",
    "format": "prettier --write .",
    "lint": "next lint --dir ./src",
    "type-check": "tsc --pretty --noEmit --pretty",
    "test": "vitest",
    "test-dev": "vitest --watch --coverage",
    "test-ci": "vitest --run --coverage",
    "test-all": "pnpm lint && pnpm type-check && pnpm test",
    "postinstall": "husky install",
    "sync-api": "generateTypesFromUrl -u https://devfriends-backend.fly.dev/-json -o ./src/api/main-backend/specs && eslint ./src/api/main-backend/specs --fix",
    "sync-icons": "pnpm generateReadmeIcons -h 50"
  },
  "engines": {
    "node": "20.x"
  },
  "dependencies": {
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@mui/icons-material": "^5.15.17",
    "@mui/material": "^5.15.17",
    "@mui/system": "^5.15.15",
    "@tanstack/react-query": "^5.36.0",
    "axios": "^1.6.8",
    "jotai": "^2.8.0",
    "next": "14.2.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "ts-pattern": "^5.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.24.5",
    "@emotion/cache": "^11.10.5",
    "@faker-js/faker": "^8.4.1",
    "@next/env": "^14.2.3",
    "@stylistic/eslint-plugin": "^2.1.0",
    "@swc/core": "^1.5.5",
    "@testing-library/dom": "^10.1.0",
    "@testing-library/jest-dom": "^6.4.5",
    "@testing-library/react": "^15.0.7",
    "@testing-library/user-event": "^14.5.2",
    "@types/node": "^20.12.11",
    "@types/react": "^18.3.2",
    "@typescript-eslint/eslint-plugin": "^7.8.0",
    "@typescript-eslint/parser": "^7.8.0",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^1.6.0",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.2.3",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-markdown": "^5.0.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-testing-library": "^6.2.2",
    "husky": "^9.0.11",
    "identity-obj-proxy": "^3.0.0",
